<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="216.0" prefWidth="444.0" styleClass="body"
            stylesheets="@../css/ChooseBaseIP.css" xmlns="http://javafx.com/javafx/8"
            fx:controller="client.controllers.ChooseBaseIPController">
    <children>
        <Label alignment="CENTER" layoutX="13.0" layoutY="32.0" prefHeight="21.0" prefWidth="413.0"
               text="Please Enter The Base IP For The Network"/>
        <TextField fx:id="IP" layoutX="30.0" layoutY="83.0" prefHeight="25.0" prefWidth="384.0"
                   promptText="Please Enter Network Base IP"/>
        <Button fx:id="search" layoutX="237.0" layoutY="120.0" mnemonicParsing="false" onAction="#onSearchButtonClicked"
                prefHeight="61.0" prefWidth="125.0" styleClass="searchButton" text="Search"/>
        <Button fx:id="cancel" layoutX="78.0" layoutY="120.0" mnemonicParsing="false" onAction="#onCancelButtonClicked"
                prefHeight="61.0" prefWidth="125.0" styleClass="cancelButton" text="Cancel"/>
    </children>
</AnchorPane>
